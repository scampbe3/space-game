{
  "name": "space_site",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron-main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve:dist": "npm run build && npx serve dist",
